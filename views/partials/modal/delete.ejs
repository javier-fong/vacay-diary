<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
aria-hidden="true">
<div class="modal-dialog" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Are you sure to delete?</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            You are unable to recover your itinerary once deleted. <br>Confirm to delete?
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            <form action="/app/my-itineraries/<%= itinerary._id %>?_method=DELETE" method="POST"
                id="deleteForm">
                <button type="submit" class="btn btn-primary" id="deleteBtn">Confirm</button>
            </form>
        </div>
    </div>
</div>
</div>